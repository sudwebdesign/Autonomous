<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Ixquick</os:ShortName>
<os:Description>Ixquick</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>https://ixquick.com/row/</SearchForm>
<os:Url type="text/html" method="POST" template="https://ixquick.com/row/search">
  <os:Param name="q" value="{searchTerms}"/>
  <os:Param name="l" value="english"/>
</os:Url><os:Url type="application/x-suggestions+json" method="GET" template="https://ixquick.com/cgi-bin/csuggest?output=json&amp;pl=ff&amp;lang=english&amp;query={searchTerms}">
</os:Url>
</SearchPlugin>
